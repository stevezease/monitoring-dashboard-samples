{
    "displayName": "kube-state-metrics-v2",
    "dashboardFilters": [
        {
            "labelKey": "cluster",
            "templateVariable": "cluster",
            "stringValue": "",
            "filterType": "RESOURCE_LABEL"
        },
        {
            "labelKey": "node",
            "templateVariable": "node",
            "stringValue": "",
            "filterType": "METRIC_LABEL"
        },
        {
            "labelKey": "namespace",
            "templateVariable": "namespace",
            "stringValue": "",
            "filterType": "RESOURCE_LABEL"
        }
