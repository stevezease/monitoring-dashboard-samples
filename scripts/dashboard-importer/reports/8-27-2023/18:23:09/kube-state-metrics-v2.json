                    "scorecard": {
                        "gaugeView": {
                            "lowerBound": 0,
                            "upperBound": 100
                        },
                        "thresholds": [],
                        "timeSeriesQuery": {
                            "prometheusQuery": "sum(kube_pod_info{cluster=~\"${cluster.value}\",node=~\"${node.value}\"}) / sum(kube_node_status_allocatable{cluster=~\"${cluster.value}\",resource=\"pods\",node=~\"${node.value}\"})"
