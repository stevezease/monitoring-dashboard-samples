                        "thresholds": [],
                        "timeSeriesQuery": {
                            "prometheusQuery": "sum(kube_pod_info{${cluster}\",${node}\"}) / sum(kube_node_status_allocatable{${cluster}\",resource=\"pods\",${node}\"})"
                        }
                    },
                    "title": "Cluster Pod Requested"
