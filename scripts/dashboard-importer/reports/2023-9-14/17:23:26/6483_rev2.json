{
    "displayName": "ElasticSearch",
    "dashboardFilters": [
        {
            "labelKey": "cluster",
            "templateVariable": "cluster",
            "stringValue": "",
            "filterType": "RESOURCE_LABEL"
        },
        {
            "labelKey": "name",
            "templateVariable": "name",
            "stringValue": "",
            "filterType": "METRIC_LABEL"
