        "widget": {
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "prometheusQuery": "process_uptime_seconds{application=~\"${application.value}\", instance=~\"${instance.value}\"}"
            }
          },
          "title": "Uptime"
