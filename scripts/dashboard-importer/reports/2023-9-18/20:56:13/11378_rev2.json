      "stringValue": "",
      "filterType": "METRIC_LABEL"
    },
    {
      "labelKey": "instance",
      "templateVariable": "instance",
      "stringValue": "",
      "filterType": "RESOURCE_LABEL"
    },
    {
      "labelKey": "pool",
      "templateVariable": "hikaricp",
      "stringValue": "",
      "filterType": "METRIC_LABEL"
    },
    {
      "labelKey": "id",
      "templateVariable": "memory_pool_heap",
      "stringValue": "",
      "filterType": "METRIC_LABEL"
