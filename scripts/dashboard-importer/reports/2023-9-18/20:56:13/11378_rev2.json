            "thresholds": [],
            "timeSeriesQuery": {
              "prometheusQuery": "sum(jvm_memory_used_bytes{application=~\"${application.value}\", instance=~\"${instance.value}\", area=\"heap\"})*100/sum(jvm_memory_max_bytes{application=~\"${application.value}\",instance=~\"${instance.value}\", area=\"heap\"})"
