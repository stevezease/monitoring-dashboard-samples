{
    "displayName": "Node Exporter / USE Method / Node",
    "dashboardFilters": [
        {
            "labelKey": "cluster",
            "templateVariable": "cluster",
            "stringValue": "",
            "filterType": "RESOURCE_LABEL"
        },
        {
            "labelKey": "instance",
            "templateVariable": "instance",
            "stringValue": "",
            "filterType": "RESOURCE_LABEL"
        }
    ],
    "mosaicLayout": {
        "columns": 24,
        "tiles": [
            {
                "widget": {
                    "title": "CPU Utilisation",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_cpu_utilisation:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Utilisation"
                            }
                        ]
                    }
                },
                "xPos": 0,
                "yPos": 1,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "CPU Saturation (Load1 per CPU)",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_load1_per_cpu:ratio{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Saturation"
                            }
                        ]
                    }
                },
                "xPos": 12,
                "yPos": 1,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "Memory Utilisation",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_memory_utilisation:ratio{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Utilisation"
                            }
                        ]
                    }
                },
                "xPos": 0,
                "yPos": 9,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "Memory Saturation (Major Page Faults)",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_vmstat_pgmajfault:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Major page Faults"
                            }
                        ]
                    }
                },
                "xPos": 12,
                "yPos": 9,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "Network Utilisation (Bytes Receive/Transmit)",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_network_receive_bytes_excluding_lo:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Receive"
                            },
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_network_transmit_bytes_excluding_lo:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Transmit"
                            }
                        ]
                    }
                },
                "xPos": 0,
                "yPos": 17,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "Network Saturation (Drops Receive/Transmit)",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_network_receive_drop_excluding_lo:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Receive"
                            },
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance:node_network_transmit_drop_excluding_lo:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "Transmit"
                            }
                        ]
                    }
                },
                "xPos": 12,
                "yPos": 17,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "Disk IO Utilisation",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance_device:node_disk_io_time_seconds:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "${labels.device}"
                            }
                        ]
                    }
                },
                "xPos": 0,
                "yPos": 25,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "Disk IO Saturation",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "instance_device:node_disk_io_time_weighted_seconds:rate5m{job=\"node-exporter\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"} != 0"
                                },
                                "legendTemplate": "${labels.device}"
                            }
                        ]
                    }
                },
                "xPos": 12,
                "yPos": 25,
                "height": 7,
                "width": 12
            },
            {
                "widget": {
                    "title": "Disk Space Utilisation",
                    "xyChart": {
                        "dataSets": [
                            {
                                "plotType": "STACKED_AREA",
                                "timeSeriesQuery": {
                                    "prometheusQuery": "sort_desc(1 -\n  (\n   max without (mountpoint, fstype) (node_filesystem_avail_bytes{job=\"node-exporter\", fstype!=\"\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"})\n   /\n   max without (mountpoint, fstype) (node_filesystem_size_bytes{job=\"node-exporter\", fstype!=\"\", instance=~\"${instance.value}\", cluster=~\"${cluster.value}\"})\n  ) != 0\n)\n"
                                },
                                "legendTemplate": "${labels.device}"
                            }
                        ]
                    }
                },
                "xPos": 0,
                "yPos": 33,
                "height": 7,
                "width": 24
            }
        ]
    },
    "labels": {
        "kube-prometheus-mixin": "",
        "goog-imported-via-script": "",
        "cloud-ops-grafana-importer": "",
        "goog-imported-grafana-dashboard": "",
        "goog-imported-grafana-revision-1": "",
        "goog-imported-grafana-file-name-node_exporter_use_node": ""
    }
}